{
    "connection": "vessel-catalog-kafka",
    "name": "vessel_catalog",
    "topic": "vessel_catalog",
    "value": {
        "vessel_id": {
            "_gen": "sequentialString",
            "startingFrom": 10,
            "expr": "VESSEL-~d"
        },
        "vessel_name": {
            "_gen": "string",
            "expr": "#{Dog.name}"
        },
        "base_zone": {
            "_gen": "oneOf",
            "choices": [
                "French Quarter",
                "Marigny",
                "Bywater",
                "Warehouse District",
                "Uptown",
                "Garden District",
                "Central Business District (CBD)"
            ]
        },
        "availability": {
            "_gen": "oneOf",
            "choices": [
                "available",
                "hired",
                "in_dock"
            ]
        },
        "capacity": {
            "_gen": "uniformDistribution",
            "bounds": [5, 10],
            "decimals": 0
        }
    },
    "localConfigs": {
        "maxEvents": 31,
        "avroSchemaHint": {
            "value": {
                "type": "record",
                "name": "vessel_catalog_value",
                "namespace": "org.apache.flink.avro.generated.record",
                "fields": [
                    {"name": "vessel_id", "type": "string"},
                    {"name": "vessel_name", "type": "string"},
                    {"name": "base_zone", "type": "string"},
                    {"name": "availability", "type": "string"},
                    {"name": "capacity", "type": "int"}
                ]
            }
        }
    }
}

